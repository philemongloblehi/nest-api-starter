{
  "name": "my-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration": "npm run typeorm migration:run",
    "generate:migration": "npm run typeorm migration:generate -- -n initial",
    "new:migration": "npm run typeorm migration:create -- -n initial",
    "update:patch": "superman version:update -p && npm run versionning",
    "update:minor": "superman version:update -i && npm run versionning",
    "update:version": "superman version:update -m && npm run versionning",
    "versionning": "node ./build/build-pre.js"
  },
  "dependencies": {
    "@nest-modules/mailer": "^1.1.3",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.9.0",
    "@nestjs/platform-socket.io": "^6.10.14",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.10.14",
    "aws-sdk": "^2.593.0",
    "bcrypt": "^3.0.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "compression": "^1.7.4",
    "config": "^3.2.4",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-mailer": "^0.3.1",
    "express-session": "^1.17.0",
    "fs": "0.0.1-security",
    "google-calendar-holiday": "^1.0.1",
    "holidayapi": "^3.0.0",
    "moment": "^2.24.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "socket.io": "^2.3.0",
    "superman-versioning": "^0.1.6",
    "swagger": "^0.7.5",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.19",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "24.0.11",
    "@types/multer": "^1.3.9",
    "@types/node": "^11.13.4",
    "@types/passport-jwt": "^3.0.2",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
